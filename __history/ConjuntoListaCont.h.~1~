//---------------------------------------------------------------------------

#ifndef ConjuntoListaContH
#define ConjuntoListaContH
#include "ClaseListaContinua.h"
//---------------------------------------------------------------------------

class Conjunto{
 private:
	  Lista elem;

 public:
	void Crear();
	bool Vacia();
	int Cardinal();
	void inserta(int elemento);
	void suprime(int elemento);
	bool pertenece(int elemento);
	int muestrea();
	void Union(Conjunto A,Conjunto B);
	void Interserccion(Conjunto A,Conjunto B);
	void No_Interseccion(Conjunto A,Conjunto B);

	void mostrar(TStringGrid *t);
};
















#endif
